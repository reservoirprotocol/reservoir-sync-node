{
  "name": "reservoir-light-node",
  "scripts": {
    "start": "yarn build && yarn prepare && node dist/index.js",
    "prepare": "yarn database && yarn flush",
    "flush": "node redis-flush.js",
    "database": "yarn database:generate && yarn database:migrate && yarn database:deploy",
    "database:generate": "npx prisma generate",
    "database:deploy": "npx prisma migrate deploy",
    "database:migrate": "npx prisma migrate reset --force",
    "build": "yarn run clean && tsc",
    "develop": "nodemon",
    "lint": "yarn run lint:fix && yarn run lint:format",
    "lint:fix": "eslint --fix . --ext .ts",
    "lint:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "yarn run test:all",
    "clean": "rm -rf dist",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "echo unit testing",
    "test:integration": "echo integration testing"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@prisma/client": "4.11.0",
    "@reservoir0x/reservoir-sdk": "^0.6.4",
    "@sentry/node": "^7.44.2",
    "@sentry/types": "^7.44.2",
    "@types/node-schedule": "^2.1.0",
    "@types/uuid": "^9.0.1",
    "axios": "^1.3.4",
    "colors": "^1.4.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-http": "^1.17.1",
    "node-cron": "^3.0.2",
    "node-fetch": "2.6.9",
    "node-schedule": "^2.1.1",
    "pg": "^8.10.0",
    "pg-promise": "^11.4.1",
    "prisma": "4.11.0",
    "redis": "^4.6.5",
    "sentry": "^0.1.2",
    "ts-node": "10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13",
    "web3": "^1.9.0",
    "winston": "^3.8.2"
  },
  "engines": {},
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@types/node-cron": "^3.0.7",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.35.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5"
  }
}
